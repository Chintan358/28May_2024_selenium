package pettern;

public class P1 {
	public static void main(String[] args) {
		
//		*****
//		*****
//		*****
//		*****
//		*****
		
//		11111
//		22222
//		33333
//		44444
//		55555
		
		
		
		int lines = 7;
		
		for(int j=1;j<=lines;j++)
		{
			for(int i=1;i<=lines;i++)
			{
				System.out.print(j);
			}
			System.out.println();
		}
		
	
		
		
		
		
	}
}
