package basic;

public class Datatypes {
	public static void main(String[] args) {
		
		
		byte b = -128;
		short s = 120;
		int i = 5666;
		long l = 56565;
		
		
		float f = 65.3325656565f;
		double d = 565656.56565656565;
		
		char ch = 'A';
		
		boolean bool = false;
		
		String str = "Tejas tadvi";
//		
//		System.out.println(f);
//		System.out.println(d);
		
		
		int x = 'D';
		System.out.println(x);
		
		char y = 65;
		System.out.println(y);
		
	}
}
