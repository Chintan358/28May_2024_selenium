package pettern;

public class P1 {
	public static void main(String[] args) {
		
//		*****
//		*****
//		*****
//		*****
//		*****
		int lines = 7;
		
		for(int j=1;j<=lines;j++)
		{
			for(int i=1;i<=lines;i++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
	
		
		
		
		
	}
}
