package oops;

class Student
{
	int id;
	String name;
	String clg;
	
	Student()
	{
		System.out.println("constructor calling...");
	}
	
	Student(int a)
	{
		System.out.println(a);
	}
	
	Student(String b)
	{
		System.out.println(b);
	}
	
	public void display()
	{
		System.out.println(id+" "+name+" "+clg);
	}
}

public class O002_constructor {
	public static void main(String[] args) {
		
		
		Student s1 = new Student("hello");
		s1.display();
		
	}
}
