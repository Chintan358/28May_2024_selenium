package operators;

public class O001_Arithmatic {
	public static void main(String[] args) {
		
		
		int a = 10;
		int b = 20;
		
		System.out.println(a+b);
		System.out.println(a+"Tops");
		System.out.println(a+b+"tops");
		System.out.println("tops"+a+b);
		
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		
		System.out.println(a%b);
	}
}
