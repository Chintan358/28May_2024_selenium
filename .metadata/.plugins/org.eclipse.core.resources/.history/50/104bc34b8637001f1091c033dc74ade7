package J004_array;

public class A001_ArrayDemo {
	public static void main(String[] args) {
		
		//int a = 10;
		
		//declaration
		int a[] = new int[10];
		
		//initialization
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		a[6] = 50;
		
		for(int i=0;i<a.length;i++)
		{
			System.out.println(a[i]);
		}
		
	}
}
