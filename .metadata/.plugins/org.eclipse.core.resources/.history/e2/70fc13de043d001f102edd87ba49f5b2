package oops;

class Student
{
	int id;
	String name;
	String clg;
	
	Student()
	{
		System.out.println("constructor calling...");
	}
	
	Student(int a)
	{
		System.out.println(a);
	}
	
	Student(int id,String name,String clg)
	{
		this.id = id;
		this.name = name;
		this.clg = clg;
	}
	
	public void display()
	{ 
		System.out.println(id+" "+name+" "+clg);
	}
}

public class O002_constructor {
	public static void main(String[] args) {
		
		
		Student student1 = new Student(10,"Nikunj","SD");
		student1.display();
		
		Student s2 = new Student(11,"Nikunj","SD");
		s2.display();
		
	}
}
